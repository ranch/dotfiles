export TERM=xterm-256color
export EDITOR=vim
export FILE=ranger
export BROWER=firefox

export GDRIVE=$HOME/Documents/gdrive
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/local/sbin:/usr/lib/jvm/default/bin:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl

# Go
export GOROOT_BOOTSTRAP=/usr/local/go1.9.5
export GOPATH=/home/randy/workspace
export GOROOT=/home/randy/go
export PATH=$PATH:$GOROOT/bin:$GOPATH/bin

# nvm
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

# Shortcuts
export GITHUB=$GOPATH/src/github.com
export GITLAB=$GOPATH/src/gitlab.com
export BITBUCKET=$GOPATH/src/bitbucket.org

export RND=$GITHUB/rnd
export RNDIR=$GITHUB/rndir

export BITROUPE=$GOPATH/src/git.bitroupe.com
export SPICY=$GITHUB/spicynoodle

export BRANKAS=$GOPATH/src/brank.as
export BRI=$GOPATH/src/gitlab.bri.co.id

export TERM=xterm-256color
export EDITOR=vim
export FILE=ranger
export BROWER=firefox

# Go
export GOROOT_BOOTSTRAP=/usr/local/go1.9.5
export GOPATH=/home/randy/workspace
export GOROOT=/home/randy/go
export PATH=$PATH:$GOROOT/bin:$GOPATH/bin

# nvm
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

# Shortcuts
export GITHUB=$GOPATH/github.com
export GITLAB=$GOPATH/gitlab.com
export BITBUCKET=$GOPATH/bitbucket.org

export RND=$GOPATH/rnd
export RNDIR=$GOPATH/rndir

export SPICY=$GOPATH/spicynoodle
export BRANKAS=$GOPATH/brank.as
